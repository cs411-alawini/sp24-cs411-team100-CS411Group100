{
  "name": "cs411-service",
  "version": "0.1.0",
  "description": "CS411 Service",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "NODE_ENV=prod forever start server.js",
    "stg": "NODE_ENV=stg forever start server.js",
    "dev": "NODE_ENV=dev&&forever start server.js",
    "local": "NODE_ENV=local&&forever start server.js",
    "localnode": "set NODE_ENV=local&&node server.js",
    "devnode": "set NODE_ENV=dev&&node server.js",
    "devnodemon": "set NODE_ENV=dev&&nodemon server.js",
    "localnodemon": "set NODE_ENV=local&&nodemon server.js",
    "localstg": "set NODE_ENV=stg&&nodemon server.js",
    "stop": "forever stop server.js",
    "stopall": "forever stopall",
    "restart": "forever restart server.js",
    "list": "forever list",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "start:win": "set NODE_ENV=dev&&node server.js",
    "start": "NODE_ENV=dev node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apidoc": "^0.53.1",
    "body-parser": "^1.20.1",
    "config.json": "0.0.4",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "dot-env": "0.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-query-boolean": "^2.0.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "moment": "^2.29.4",
    "mongodb": "^4.12.0",
    "mongoose": "^6.7.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nconf": "^0.12.0",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.20",
    "oauth2orize": "^1.11.1",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-oauth2": "^1.6.1",
    "passport-oauth2-client-password": "^0.1.2",
    "passport-oauth2-middleware": "^1.0.3",
    "passport-okta-oauth": "^0.0.1",
